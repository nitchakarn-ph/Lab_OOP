package lab1;

public class main {

	public static void main(String[] args) {
		int var1 = 31; //เก็บข้อมูล same C 
		double var2 = 3.21;
		String var3 = "Java"; // S ใหญ่เสมอ 
		System.out.println(var3); //printf
		
		int[] arr1 = new int[5]; //make to object  for use in array // array
		arr1[0]=11;
		arr1[1]=22;
		arr1[2]=33;
		arr1[3]=15;
		arr1[4]=9;
		
		int[] arr2 = {101,102,103}; //array
		
		//for loop
		System.out.println(arr1.length);//กำหนดขนาดของ array
		for (int i=0;i<arr1.length;i++)
		{
			//11 15
			if(i=0 ) {
			System.out.println(arr1[i]);
			
			}
		}
		
	}

}
